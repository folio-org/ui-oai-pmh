{
  "name": "@folio/oai-pmh",
  "version": "1.0.0",
  "description": "OAI-PMH manager",
  "main": "src/index.js",
  "repository": "folio-org/ui-oai-pmh",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test karma",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.8.1",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^5.2.0",
    "@folio/stripes": "^3.0.0",
    "@folio/stripes-cli": "^1.15.0 || ^1.14.0",
    "@folio/stripes-components": "^6.2.0",
    "@folio/stripes-core": "^4.0.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.1",
    "eslint": "^6.2.1",
    "mocha": "^6.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.6.0",
    "react-final-form": "^6.3.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^3.0.0",
    "core-js": "^3.6.1",
    "react": "*",
    "react-intl": "^2.9.0",
    "react-router-dom": "^4.3.1"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "type": "settings",
    "displayName": "ui-oai-pmh.meta.title",
    "route": "/oaipmh",
    "hasSettings": true,
    "okapiInterfaces": {},
    "permissionSets": [
      {
        "permissionName": "ui-oai-pmh.view",
        "displayName": "Display list of settings pages - user can view all oai-pmh settings, but \"Save\" button is disabled",
        "subPermissions": [
          "settings.enabled",
          "configuration.entries.collection.get",
          "configuration.entries.item.put"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-oai-pmh.edit",
        "displayName": "Can view and edit settings - user can view all oai-pmh settings and \"Save\" button is enabled if field is modified",
        "subPermissions": [
          "settings.enabled",
          "configuration.entries.collection.get",
          "configuration.entries.item.put"
        ],
        "visible": true
      }
    ]
  }
}
